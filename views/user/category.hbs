{{>user-navbar}}
<form id="brandFilter" method="post">
<section class="cat_product_area section_padding">
    <div class="container">
        <div class="row">
            <div class="col-lg-3">
                <div class="left_sidebar_area">
                    <aside class="left_widgets p_filter_widgets">

                        
                        <div class="l_w_title">
                            <h3>Browse Categories</h3>
                        </div>
                        <div class="widgets_inner">
                            <ul class="list">
                                <!-- Default checkbox -->
                                {{#each category}}
                                <label for="s_sm" class="d-flex">
                                    <input type="radio" id="brands" name="brandName" value=""
                                        class="mr-2 mt-1"> <span class="text-black"><a href="/getsubcategory/{{this._id}}">{{this.name}}</a>
                                    </span>
                                </label>
                                {{/each}}
                                <!-- Checked checkbox -->
                            </ul>
                        </div>
                    </aside>

                    <aside class="left_widgets p_filter_widgets">
                        <div class="l_w_title">
                            <h3>Product filters</h3>
                        </div>
                        <div class="widgets_inner">
                            <ul class="list">
                                {{#each response}}
                                <label for="s_sm" class="d-flex">
                                    <input type="radio" id="brands" name="brandName" value="{{this.name}}"
                                        class="mr-2 mt-1"> <span class="text-black">{{this.name}}
                                    </span>
                                </label>
                                {{/each}}
                            </ul>
                        </div>
                    </aside>

                    


                  
                </div>
            </div>
            <div class="col-lg-9">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="product_top_bar d-flex justify-content-between align-items-center">
                            <div class="single_product_menu d-flex">
                                <h5>show :</h5>
                                <div class="top_pageniation">
                                    <ul>
                                        <li>1</li>
                                        <li>2</li>
                                        <li>3</li>
                                    </ul>
                                </div>
                            </div>
                            <div class="single_product_menu d-flex">
                                <form action="/category" method="post">
                                    <div class="input-grou">
                                        <input type="search" name="search" class="form-control rounded"
                                            placeholder="Search" aria-label="Search" aria-describedby="search-addon" />
                                        <button type="submit" class="button-73">search</button>
                                    </div>
                                </form>

                            </div>
                        </div>
                    </div>
                </div>

                <div class="row align-items-center latest_product_inner">
                    {{#each data}}
                    <div class="col-lg-3 col-sm-6">
                        <div class="single_product_item">
                            <a href="/productShow/{{this._id}}"><img src="/uploads/{{this.images.[0].img1}}" alt=""></a>
                            <div class="single_product_text">
                                <div class="d-flex justify-content-between">
                                    <h4 style="font-weight: 300;">{{this.productName}}</h4>
                                    <button onclick="addingTofav('{{this._id}}')"
                                        style="background-color: transparent; border:none; font-size: 1.5em;"
                                        class=""><i class="fa fa-heart-o" aria-hidden="true"></i></button>
                                </div>
                                <h3 style="font-weight: bolder"> â‚¹ {{this.price}}</h3>
                                <div class="">
                                    <button onclick="addingToCart('{{this._id}}')" class="button-86">cart</button>


                                </div>
                                {{!-- <button class="button-2" role="button">Button 2</button> --}}
                                {{!-- <button class="button-42" role="button">+ add to wishlist</button> --}}
                                {{!-- <i class="ti-heart"></i> --}}
                            </div>
                        </div>
                    </div>
                    {{/each}}
                    <div class="col-lg-12">
                        <div class="pageination">
                            <nav aria-label="Page navigation example">
                                <ul class="pagination justify-content-center">
                                    <li class="page-item">
                                        <a class="page-link" href="#" aria-label="Previous">
                                            <i class="ti-angle-double-left"></i>
                                        </a>
                                    </li>
                                    <li class="page-item"><a class="page-link" href="#">1</a></li>
                                    <li class="page-item"><a class="page-link" href="#">2</a></li>
                                    <li class="page-item"><a class="page-link" href="#">3</a></li>
                                    <li class="page-item"><a class="page-link" href="#">4</a></li>
                                    <li class="page-item"><a class="page-link" href="#">5</a></li>
                                    <li class="page-item"><a class="page-link" href="#">6</a></li>
                                    <li class="page-item">
                                        <a class="page-link" href="#" aria-label="Next">
                                            <i class="ti-angle-double-right"></i>
                                        </a>
                                    </li>
                                </ul>
                            </nav>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
</form>


